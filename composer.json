{
    "name": "werkspot/sitemap-bundle",
    "description": "Bundle for generating dynamic sitemap.xml content with support for multiple sections",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": [
        "seo",
        "sitemap",
        "symfony",
        "bundle"
    ],
    "autoload": {
        "psr-4": {
            "Werkspot\\Bundle\\SitemapBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Werkspot\\Bundle\\SitemapBundle\\Tests\\": "Tests"
        },
        "classmap": ["Tests/Controller/app/AppKernel.php"]
    },
    "authors": [
        {
            "name": "Thanos Polymeneas",
            "email": "zinbiel@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/framework-bundle": "^4.1"
    },
    "require-dev": {
        "ext-simplexml": "*",
        "mockery/mockery": "^1.0@dev",
        "phpunit/phpunit": "^5.7",
        "symfony/finder": "^4.1",
        "symfony/asset": "^4.1",
        "symfony/templating": "^4.1",
        "symfony/browser-kit": "^4.1",
        "symfony/twig-bridge": "^4.1",
        "symfony/twig-bundle": "^4.1"
    },
    "config": {
        "bin-dir": "vendor/bin"
    }
}
