{
    "name": "werkspot/sitemap-bundle",
    "description": "Bundle for generating dynamic sitemap.xml content with support for multiple sections",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": [
        "seo",
        "sitemap",
        "symfony",
        "bundle"
    ],
    "autoload": {
        "psr-4": { "Werkspot\\Bundle\\SitemapBundle\\": "" },
        "classmap": ["Tests/Controller/app/AppKernel.php"],
        "exclude-from-classmap": ["/Tests/"]
    },
    "autoload-dev": {
        "psr-4": { "Werkspot\\Bundle\\SitemapBundle\\Tests\\": "Tests" }
    },
    "authors": [
        {
            "name": "Thanos Polymeneas",
            "email": "zinbiel@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=7.3",
        "symfony/framework-bundle": "^v4.3.9"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.0",
        "phpunit/phpunit": "^7.5.17",
        "symfony/finder": "~2.3|~3.0|^4.0",
        "symfony/asset": "~2.3|~3.0|^4.0",
        "symfony/templating": "~2.3|~3.0|^4.0",
        "symfony/browser-kit": "~2.3|~3.0|^4.0",
        "symfony/twig-bridge": "~2.3|~3.0|^4.0",
        "symfony/twig-bundle": "~2.3|~3.0|^4.0",
        "symfony/phpunit-bridge": "^v4.4.1",
        "ext-simplexml": "*"
    },
    "config": {
        "bin-dir": "vendor/bin"
    }
}
